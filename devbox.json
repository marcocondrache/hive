{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "go-task@3.44.1",
    "helmfile@1.1.3",
    "_1password-cli@2.31.1",
    "kubectl@1.33.3",
    "fluxcd@2.6.4",
    "kubernetes-helm@3.18.4",
    "kubernetes-helmPlugins.helm-diff@3.12.4",
    "cilium-cli@0.18.6",
    "minijinja@2.11.0",
    "tpi@1.0.7",
    "github:budimanjojo/talhelper",
    "kubefetch@0.8.1",
    "ipmitool@1.8.19",
    "talosctl@1.10.5",
    "yq-go@4.47.1",
    "arping@latest",
    "kubectl-node-shell@1.11.0"
  ],
  "env": {
    "HELM_PLUGINS": "$PWD/.devbox/nix/profile/default",
    "KUBECONFIG": "$PWD/kubeconfig",
    "TALOSCONFIG": "$PWD/kubernetes/talos/clusterconfig/talosconfig",
    "ROOT_DIR": "$PWD"
