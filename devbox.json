{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "go-task@3.43.3",
    "helmfile@1.1.2",
    "_1password-cli@2.31.1",
    "kubectl@1.33.1",
    "fluxcd@2.5.1",
    "kubernetes-helm@3.18.2",
    "kubernetes-helmPlugins.helm-diff@3.12.2",
    "cilium-cli@0.18.4",
    "minijinja@2.10.2",
    "tpi@1.0.7",
    "github:budimanjojo/talhelper",
    "kubefetch@0.8.1",
    "ipmitool@1.8.19",
    "talosctl@1.10.3",
    "yq-go@4.45.4",
    "arping@latest"
  ],
  "env": {
    "HELM_PLUGINS": "$PWD/.devbox/nix/profile/default",
    "KUBECONFIG": "$PWD/kubeconfig",
    "TALOSCONFIG": "$PWD/kubernetes/talos/clusterconfig/talosconfig",
    "ROOT_DIR": "$PWD"
  },
  "env_from": ".env",
  "shell": {
    "init_hook": [],
    "scripts": {}
  }
}